generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CDKey {
  id       Int     @id @default(autoincrement())
  key      String  // Stored as base64-encoded encrypted string
  tokenId  BigInt? @unique // Links to NFT tokenId after assignment
  claimed  Boolean @default(false)
}